				<eqn>NIMBY_People/Amount_of_NIMBYism_to_Cause_Problems</eqn>
				<units>Dimensionless</units>
			</aux>
			<aux name="Effect of the NIMBY\nRatio on Site Selection">
				<eqn>Ratio_of_NIMBY_People_to_Problem_Level</eqn>
				<gf>
					<xscale min="0" max="1"/>
					<yscale min="0" max="0.1"/>
					<ypts>0,0.001263,0.004341,0.01165,0.02676,0.05,0.07324,0.08835,0.09566,0.09874,0.1</ypts>
