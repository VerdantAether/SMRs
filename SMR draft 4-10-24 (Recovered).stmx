			</aux>
			<group name="Site Selection" run="false">
				<entity name="Ratio_of_total_plants_to_sites_selected"/>
				<entity name='"Effect_of_ratio_of_sites_on_NIMBY-ism"'/>
				<entity name="Sites_selected"/>
				<entity name="Effect_of_Ratio_on_new_sites_selected"/>
				<entity name="Sites_Cancelled"/>
				<entity name="Desired_number_of_sites"/>
				<entity name="Adjustment_time_for_site_selection"/>
